{
    "name": "aesislabs/odoo-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle to manage your Odoo databases.",
    "keywords": ["odoo", "api", "client", "bundle", "dbal", "orm", "database", "persistence", "object"],
    "license": "MIT",
    "authors": [
        {
            "name": "Joanis ROUANET",
            "email": "joanis.ang3@gmail.com",
            "homepage": "https://github.com/Aesislabs"
        }
    ],
    "autoload": {
        "psr-4": {
            "Aesislabs\\Bundle\\OdooBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ang3/php-odoo-api-client": "^6.1",
        "ang3/php-odoo-orm": "^0.1",
        "symfony/cache": ">=6.1",
        "symfony/console": ">=6.1",
        "symfony/dependency-injection": ">=6.1",
        "symfony/config": ">=6.1",
        "symfony/http-kernel": ">=6.1",
        "symfony/security-core": ">=6.1",
        "symfony/validator": ">=6.1",
        "symfony/expression-language": ">=6.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/phpunit-bridge": "^v6.0"
    }
}
